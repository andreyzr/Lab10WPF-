<Window x:Class="_04_Shape.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_04_Shape"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="400 ">
    <StackPanel>
        <Ellipse Fill="LightBlue" Width="200" Height="200" StrokeThickness="5" StrokeDashArray="4 2"
                 Stroke="Gray" StrokeDashCap="Round"/>
        <Rectangle Fill="LightPink" Width="200" Height="100" RadiusX="15" RadiusY="15" Margin="10" />
        <Line X1="100" Y1="200" Y2="150" Stroke="Red"/>
        <Polyline Stroke="Red" Points="100,150,200,50,300,150"/>
        <Polygon Fill="LightBlue" Points="100,150,200,50,300,150"/>
    </StackPanel>
</Window>
